# IMPORTANT: Set these in Render.com dashboard, not in this file!
# This file is just a reference for what variables you need to set

# Access password for the app (CHANGE THIS!)
ACCESS_PASSWORD=your_secure_password_here

# Session secret (generate a random string)
SESSION_SECRET=generate_a_random_string_min_32_characters

# Node environment
NODE_ENV=production

# Port (Render sets this automatically)
PORT=10000
